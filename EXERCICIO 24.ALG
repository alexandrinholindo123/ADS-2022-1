Algoritmo "Exercicio 24"

Var
   QR,QD,QML,QLE : real

Inicio
      escreva ("Quantidade em reais: ")
      leia (QR)
      QD<- QR*1.8
      QML<-QR*2
      QLE<-QR*3.57
      escreval("convertido em dolar:",QD)
      escreval ("convertido em marco alemao:",QML)
      escreval ("convertido em libras:",QLE)
Fimalgoritmo